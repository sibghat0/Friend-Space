<div class="container mt-5 text-center">
<%= link_to "New task", new_tasker_path %> </br>
<%= month_calendar(events: @taskers) do |date, taskers| %>
    <div class="dayCont">
        <%= date.day%>
        <% taskers.each do |tasker| %>
        <div class="card-header-cal">
            <p class="cardTitle">
                <%= link_to tasker.title, tasker %>
            </p>
        </div>
        <%end%>
    </div>
<%end%>
</div>


<% @taskers.each do |tasker| %>
    <p>
      <%= link_to  tasker do %>
       <div class="container mt-5 text-left">
         <%= tasker.title%> ||
        <%= tasker.start_time.strftime('%v %R %P')%>
       </div>
      <%end%>
    </p>
  <% end %>